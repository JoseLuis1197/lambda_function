{
  "configurations": [
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "getBook:HelloWorldFunction",
      "invokeTarget": {
        "target": "template",
        "templatePath": "${workspaceFolder}/getBook/template.yaml",
        "logicalId": "HelloWorldFunction"
      },
      "lambda": {
        "payload": {
          "path": "getBook/events/event.json"
        },
        "environmentVariables": {}
      }
    },
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "listBooks:ListBooksFunction",
      "invokeTarget": {
        "target": "template",
        "templatePath": "${workspaceFolder}/listBooks/template.yaml",
        "logicalId": "ListBooksFunction"
      },
      "lambda": {
        "payload": {
          "path": "listBooks/events/event.json"
        },
        "environmentVariables": {}
      }
    }
  ]
}