{
  "configurations": [
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "invokeTarget": {
        "target": "template",
        "logicalId": "HelloWorldFunction",
        "templatePath": "${workspaceFolder}/template.yaml"
      },
      "lambda": {
        "runtime": "python3.9"
      },
      "sam": {
        "containerBuild": false,
        "skipNewImageCheck": false
      },
      "api": {
        "path": "/hello",
        "httpMethod": "get"
      },
      "name": "debugpython"
    },
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "lambda-python-test:HelloWorldFunction",
      "invokeTarget": {
        "target": "template",
        "templatePath": "${workspaceFolder}/template.yaml",
        "logicalId": "HelloWorldFunction"
      },
      "lambda": {
        "payload": {},
        "environmentVariables": {}
      }
    }
  ]
}